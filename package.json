{
  "name": "lxsidechain-sc",
  "version": "0.0.1",
  "description": "LaborX Sidechain Base Contracts",
  "main": "truffle.js",
  "dependencies": {
    "truffle": "^4.1.12",
    "ganache-cli": "6.1.3",
    "truffle-hdwallet-provider": "mikefluff/truffle-hdwallet-provider#7a2eccca1ba17c235a3ef258914ca74ea5f13014",
    "solidity-storage-lib": "chronobank/solidity-storage-lib",
    "solidity-shared-lib": "github:alesanro/solidity-shared-lib#feature/update-to-latest"
  },
  "devDependencies": {
    "abi-decoder": "^1.1.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-register": "^6.26.0",
    "minimist": "^1.2.0",
    "sha256": "^0.2.0",
    "solidity-coverage": "^0.5.4"
  },
  "scripts": {
    "test:all": "./node_modules/truffle/build/cli.bundled.js test",
    "run:rpc": "./node_modules/ganache-cli/build/cli.node.js -g 1 --gasLimit 4700000 --trace_request true | grep -Ev \"FilterSubprovider|eth_getFilterChanges\"",
    "compile": "./node_modules/truffle/build/cli.bundled.js compile",
    "compile:all": "./node_modules/truffle/build/cli.bundled.js compile --all",
    "migrate": "./node_modules/truffle/build/cli.bundled.js migrate",
    "migrate:reset": "./node_modules/truffle/build/cli.bundled.js migrate --reset",
    "console": "./node_modules/truffle/build/cli.bundled.js console --network development"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChronoBank/LXsidechain-sc.git"
  },
  "keywords": [
    "LaborX",
    "Chronobank",
    "Solidity",
    "Ethereum"
  ],
  "author": "LaborX PTY",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/ChronoBank/LXsidechain-sc/issues"
  },
  "homepage": "https://github.com/ChronoBank/LXsidechain-sc#readme"

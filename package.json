{
  "name": "@laborx/sidechain-sc",
  "version": "0.0.3",
  "description": "LaborX Sidechain Base Contracts",
  "main": "truffle.js",
  "scripts": {
    "test": "npx truffle test",
    "coverage": "./node_modules/.bin/solidity-coverage",
    "compile": "npx truffle compile",
    "compile:all": "npx truffle compile --all",
    "migrate": "npx truffle migrate",
    "migrate:reset": "npx truffle migrate --reset",
    "console": "npx truffle console --network development",
    "testrpc": "npx ganache-cli -g 1 --gasLimit 4700000 --trace_request true | grep -Ev \"FilterSubprovider|eth_getFilterChanges\""
  },
  "dependencies": {
    "truffle": "4.1.13",
    "ganache-cli": "6.1.4",
    "truffle-hdwallet-provider": "mikefluff/truffle-hdwallet-provider#7a2eccca1ba17c235a3ef258914ca74ea5f13014",
    "solidity-shared-lib": "chronobank/solidity-shared-lib#v0.2.2",
    "solidity-eventshistory-lib": "chronobank/solidity-eventshistory-lib#v0.1.1",
    "solidity-storage-lib": "chronobank/solidity-storage-lib#v0.2.1"
  },
  "devDependencies": {
    "abi-decoder": "^1.1.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-register": "^6.26.0",
    "minimist": "^1.2.0",
    "sha256": "^0.2.0",
    "solidity-coverage": "^0.5.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChronoBank/LXsidechain-sc.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "LaborX",
